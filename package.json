{
  "name": "{{projectName}}",
  "version": "1.0.0",
  "description": "{{description}}",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --config scripts/webpack.config.local.js",
    "start:mock": "webpack-dev-server --config scripts/webpack.config.local.js",
    "build:beta": "rimraf cdn &&  webpack --config scripts/webpack.config.beta.js --progress",
    "build:prod": "rimraf cdn &&  webpack --config scripts/webpack.config.prod.js --progress",
    "lint": "eslint --ext .tsx,.ts --fix ./src --quiet",
    "precommit": "lint-staged",
    "mock": "cybmock start",
    "analyzer": "webpack --config scripts/webpack.config.analyzer.js",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test": "jest --config ./jest.config.js  --BABEL_ENV=test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.(ts|tsx)": [
      "prettier --write",
      "npm run lint",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "{{author}}",
  "license": "MIT",
  "dependencies": {
    "@storybook/addon-knobs": "^5.3.19",
    {{#if antd}}
    "antd": "^4.2.5",
    {{/if}}
