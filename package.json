    "fetch-jsonp": "^1.1.3",
    "lodash": "^4.17.15",
    "mathjs": "^7.0.1",
    "numeral": "^2.0.6",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    {{#if redux}}
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "iron-redux": "^0.2.7",
    {{/if}}
    "react-router-dom": "^5.2.0",
    "whatwg-fetch": "^3.0.0"
    "@umijs/hooks": "^1.9.3",
  },
  "devDependencies": {
    "@babel/core": "^7.10.1",
